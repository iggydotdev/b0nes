{
  "name": "b0nes",
  "version": "0.2.0",
  "type": "module",
  "description": "Zero-dependency component library and SSR/SSG framework",
  "main": "src/index.js",
  "exports": {
    ".": "./src/index.js",
    "./components": "./src/components/index.js",
    "./atoms": "./src/components/atoms/index.js",
    "./molecules": "./src/components/molecules/index.js",
    "./organisms": "./src/components/organisms/index.js",
    "./framework": "./src/framework/index.js",
    "./compose": "./src/framework/core/compose.js",
    "./router": "./src/framework/core/router/index.js",
    "./utils": "./src/components/utils/index.js",
    "./urlPattern": "./src/framework/shared/urlPattern.js"
  },
  "bin": {
    "b0nes": "bin/create.js"
  },
  "scripts": {
    "test": "node --test 'src/**/*.test.js'",
    "dev": "NODE_ENV=development node src/framework/server/index.js",
    "dev:watch": "NODE_ENV=development node --watch src/framework/server/index.js",
    "build": "node src/framework/build/cli.js build",
    "build:verbose": "node src/framework/build/cli.js build --verbose",
    "build:clean": "node src/framework/build/cli.js build --clean",
    "build:fast": "node src/framework/build/cli.js build --parallel",
    "build:production": "node src/framework/build/cli.js build --clean --parallel --production",
    "clean": "node src/framework/build/cli.js clean",
    "serve": "npx serve public",
    "generate": "node src/components/utils/generator/index.js",
    "install-component": "node src/scripts/install-component.js",
    "create": "node create/index.js",
    "prepublishOnly": "npm run test"
  },
  "keywords": [
    "components",
    "ssr",
    "ssg",
    "atomic-design",
    "framework",
    "zero-dependency",
    "server-side-rendering",
    "static-site-generator",
    "web-components",
    "html",
    "no-build",
    "vanilla-js"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "author": {
    "name": "Ignacio Garcia Villanueva",
    "email": "iggy.dev@pm.me"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iggydotdev/b0nes.git"
  },
  "bugs": {
    "url": "https://github.com/iggydotdev/b0nes/issues"
  },
  "homepage": "https://github.com/iggydotdev/b0nes#readme",
  "license": "MIT"
}